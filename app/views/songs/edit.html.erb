<h1> Edit Song Form: </h1>
<%=form_for(@song) do |f|%>
    <label>My song name:</label>
    <%= f.text_field :name %><br>
    <p></p>
    <label>Artist id:</label>
    <%= f.text_field :artist_id %><br>
    <p></p>
    <label>Genre id::</label>
    <%= f.text_field :genre_id %><br>
    <p></p>
    <%= f.submit "Update Song"%>
<%end%>